{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "outDir": "./target",
    "types": [
      "node"
    ],
    "experimentalDecorators": true, //启用注解
    "emitDecoratorMetadata": true, //启用元数据
    "moduleResolution": "node", //使用node作为模块解析
    "strict": true, //启用严格模式
  }
}